.white_wrap{
	margin:0 5% 11px;
	padding:7px 0px;
}
.white_wrap table tr td:first-child{
	padding-right:20px;
	width:126px;
}
.white_wrap table{
	width:100%;
}
.white_wrap table tr td{
	vertical-align:middle;
	text-align:left;
}
.white_wrap .app_feature_list{
	float:none;
	width:auto;
}
.white_wrap .app_feature_list label{
	font-size:26px;
}